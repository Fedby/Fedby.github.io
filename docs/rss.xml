int  fb(int n){&#13;
    int a[1000001];&#13;
    a[1]=1;&#13;
    a[2]=1;&#13;
    for(int i=3;i&lt;=n;i++){&#13;
        a[i]=a[i-1]+a[i-2];&#13;
        if(a[i]&gt;=10007){&#13;
            a[i]-=10007;&#13;
        }&#13;
    }&#13;
    cout&lt;&lt;a[n];&#13;
}&#13;
int main() {&#13;
    int n;&#13;
    cin&gt;&gt;n;&#13;
    fb(n);&#13;
